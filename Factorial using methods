import java.util.*;
public class Main
{
static int Methoda(int a)
{
    if(a<=1)
    return 1;
    else 
    return a*Methodb(a-1) ;
}
static int Methodb(int a)
{
    if(a<=1)
    return 1;
    else 
    return a*Methoda(a-1) ;
}
public static void main (String[] args) {
    Scanner sc=new Scanner(System.in);
    int a=sc.nextInt();
    System.out.print(Methoda(a));
}
}
